{
  "presets": [["es2015", { "modules": false }], "stage-1"],
  "plugins": [
    "transform-class-properties",
    "transform-runtime",
    "transform-flow-strip-types",
    "syntax-flow",
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs"]
      ]
    }
  }
}
